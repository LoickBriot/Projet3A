package controllers.application    

import java.io.File

class textImageReader {
  // --- Native methods
  @native def getText(imagePath: String): String
}

object textImageReader {
  
  val rootPath: String = System.getProperties().get("user.dir").toString()

   

  def getText(imagePath: String) : String = {
      val reader = new textImageReader
      var text = reader.getText(imagePath)
      return text
  }
  
}


